#!/bin/sh

##### !!! THIS SCRIPT MUST BE OVERRIDEN IN OS-DEPENDENT IMPLEMENTATION !!! #####

GetRamBytes(){
	sysctl -nq hw.realmem || sysctl -nq hw.physmem
}


case "$0" in
	*/myx.common/bin/os/getRamBytes) 
		if [ "$1" = "--help" ] ; then
			echo "Syntax: myx.common os/getRamBytes" >&2
			exit 1
		fi
		set -e
		GetRamBytes "$@"
	;;
esac
